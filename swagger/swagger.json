{
  "swagger": "2.0",
  "info": {
      "title": "Sample API",
      "description": "API Description For User",
      "version": "1.0.0"
  },
  "host": "localhost:3000",
  "basePath": "/",
  "schemes": [
      "http"
  ],
  "consumes": [
      "application/json"
  ],
  "produces": [
      "application/json"
  ],
  "paths": {
      "/register": {
          "post": {
              "summary": "New user registration",
              "parameters": [
                  {
                      "in": "body",
                      "name": "Register",
                      "schema": {
                          "$ref": "#/definitions/User"
                      },
                      "description": "Register new user using post"
                  }
              ],
              "responses": {
                  "200": {
                      "description": "User has been successfully register"
                  }
              }
          }
      }
    },
      "definitions": {
          "User": {
              "type": "object",
              "properties": {
                  "fName": {
                      "type": "string"
                  },
                  "lName": {
                      "type": "string"
                  },
                  "email": {
                      "type": "string"
                  },
                  "password": {
                      "type": "string"
                  },
                  "confirmPassword": {
                      "type": "string"
                  }
              }
          }    
      }
  }